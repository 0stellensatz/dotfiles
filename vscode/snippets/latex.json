{
  "Standalone tikz-cd": {
    "prefix": ["tikzcdsa"],
    "body": [
      "\\documentclass[crop,tikz]{standalone}",
      "\\usepackage{tikz-cd}",
      "",
      "\\begin{document}",
      "",
      "\\begin{tikzcd}",
      "$0",
      "\\end{tikzcd}",
      "",
      "\\end{document}"
    ],
    "description": "Standalone tikz-cd."
  },
  "Preamble: Korean": {
    "prefix": ["preko"],
    "body": [
      "\\documentclass[10pt,a4paper]{article}",
      "",
      "\\input{./preamble-ko.tex}",
      "",
      "%\\usepackage{fapapersize}",
      "%\\usefapapersize{*,*,25mm,*,25mm,*}",
      "\\usepackage{geometry}",
      "\\newgeometry{hmargin={25mm}, vmargin={25mm}}",
      "",
      "\\title{${1:title}}",
      "\\author{${2:author}}",
      "",
      "\\begin{document}",
      "",
      "\\maketitle",
      "\\tableofcontents",
      "",
      "$0",
      "",
      "\\end{document}"
    ],
    "description": "Preamble: Korean, with ./preamble-ko.tex file."
  },
  "Preamble: Japanese": {
    "prefix": ["preja"],
    "body": [
      "\\documentclass[10pt,a4paper]{jsarticle}",
      "",
      "\\input{./preamble-ja.tex}",
      "",
      "\\title{${1:title}}",
      "\\author{${2:author}}",
      "",
      "\\begin{document}",
      "",
      "\\maketitle",
      "\\tableofcontents",
      "",
      "$0",
      "",
      "\\end{document}"
    ],
    "description": "Preamble: Japanese, with ./preamble-ja.tex file."
  },
  "Preamble: English": {
    "prefix": ["preen"],
    "body": [
      "\\documentclass[10pt,a4paper]{article}",
      "",
      "\\input{./preamble-en.tex}",
      "",
      "%\\usepackage{fapapersize}",
      "%\\usefapapersize{*,*,25mm,*,25mm,*}",
      "\\usepackage{geometry}",
      "\\newgeometry{hmargin={25mm}, vmargin={25mm}}",
      "",
      "\\title{${1:title}}",
      "\\author{${2:author}}",
      "",
      "\\begin{document}",
      "",
      "\\maketitle",
      "\\tableofcontents",
      "",
      "$0",
      "",
      "\\end{document}"
    ],
    "description": "Preamble: English, with ./preamble-en.tex file."
  },
  "Block Environment": {
    "prefix": ["blck"],
    "body": [
      "\\begin{${1:environment}}",
      "    $2",
      "\\end{${1:environment}}$0"
    ],
    "description": "Block Environment."
  },
  "Inline Math": {
    "prefix": ["ss"],
    "body": ["$$1$$0"],
    "description": "Inline Math."
  },
  "Superscript": {
    "prefix": ["sp"],
    "body": ["^{$1}$0"],
    "description": "Superscript."
  },
  "Subscript": {
    "prefix": ["sb"],
    "body": ["_{$1}$0"],
    "description": "Superscript."
  },
  "\\left( \\right)": {
    "prefix": ["p1"],
    "body": ["\\left( $1 \\right)$0"],
    "description": "\\left( \\right)"
  },
  "\\left\\{ \\right\\}": {
    "prefix": ["p2"],
    "body": ["\\left\\{ $1 \\right\\\\}$0"],
    "description": "\\left\\{ \\right\\}"
  },
  "\\left[ \\right]": {
    "prefix": ["p3"],
    "body": ["\\left[ $1 \\right]$0"],
    "description": "\\left[ \\right]"
  },
  "\\langle \\rangle": {
    "prefix": ["p4"],
    "body": ["\\langle $1 \\rangle$0"],
    "description": "\\langle \\rangle"
  }
}