{
  "window.zoomLevel": 0,

  "editor.fontFamily": "Iosevka Cherrypick",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "editor.tabCompletion": true,
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  "editor.rulers": [ 80, 120 ],

  "terminal.integrated.fontSize": 15,
  "terminal.integrated.letterSpacing": 1,
  "terminal.external.linuxExec": "konsole",

  "diffEditor.ignoreTrimWhitespace": true,
  "git.autofetch": true,

  "keyboard.dispatch": "keyCode",

  "latex-workshop.latex.recipes": [
    {
      "name": "platex",
      "tools": [
        "platex",
        "dvipdfmx"
      ]
    },
    {
      "name": "latex",
      "tools": [
        "latex",
        "dvipdfmx"
      ]
    },
    {
      "name": "xelatex",
      "tools": [
        "xelatex"
      ]
    },
    {
      "name": "pdflatex -> bibtex -> pdflatex*2",
      "tools": [
        "pdflatex",
        "bibtex",
        "pdflatex",
        "pdflatex"
      ]
    },
    {
      "name": "uplatex",
      "tools": [
        "uplatex",
        "dvipdfmx"
      ]
    },
    {
      "name": "platex -> pbibtex -> platex*2 -> dvipdfmx",
      "tools": [
        "platex_doc",
        "pbibtex",
        "platex_doc",
        "platex_doc",
        "dvipdfmx"
      ]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "name": "latex",
      "command": "latex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOCFILE%"
      ]
    },
    {
      "name": "xelatex",
      "command": "xelatex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "bibtex",
      "command": "bibtex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "platex",
      "command": "platex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "platex_doc",
      "command": "platex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "uplatex",
      "command": "uplatex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "dvipdfmx",
      "command": "dvipdfmx",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "pbibtex",
      "command": "pbibtex",
      "args": [
        "%DOCFILE%"
      ]
    }
  ],
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.view.pdf.viewer": "tab",

  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["h"],
      "after": ["k"]
    },
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": [":"],
      "after": [";"]
    }
  ]
}
  