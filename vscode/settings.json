  "vim.autoSwitchInputMethod.obtainIMCmd": "\\Users\\Public\\im-select\\im-select.exe",
  "vim.autoSwitchInputMethod.switchIMCmd": "\\Users\\Public\\im-select\\im-select.exe {im}",

