      ]
    },
    {
      "name": "upbibtex",
      "command": "upbibtex",
      "args": [
        "%DOCFILE%"
      ]
    },
    {
      "name": "rclone_nextcloud",
      "command": "rclone",
      "args": [
        "sync",
        "%DOCFILE%.pdf",
        "Nextcloud:/"
      ]
    }
  ],
  "latex-workshop.latex.recipe.default": "lastUsed",
  "latex-workshop.view.pdf.viewer": "tab",

  "vim.easymotion": true,
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.smartRelativeLine": true,
  "vim.useCtrlKeys": false,
  "vim.useSystemClipboard": true,

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["h"],
      "after": ["k"]
    },
    {
      "before": ["g", "k"],
      "after": ["g", "j"]
    },
    {
      "before": ["g", "h"],
      "after": ["g", "k"]
    },
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": [":"],
      "after": [";"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["h"],
      "after": ["k"]
    },
    {
      "before": ["g", "k"],
      "after": ["g", "j"]
    },
    {
      "before": ["g", "h"],
      "after": ["g", "k"]
    },
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": [":"],
      "after": [";"]
    }
  ],

